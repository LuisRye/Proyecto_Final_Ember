<div class="container">
        <div class="row centered-form">
        <div class="col-xs-12 col-sm-8 col-md-4 col-sm-offset-2 col-md-offset-4">
        	<div class="panel panel-default">
        		<div class="panel-heading">
			    		<h3 class="panel-title">Vista Eventos<small></small></h3>
			 			</div>
			 			<div class="panel-body">
			    		<form role="form" {{action "saveData" model on="submit"}}>
			    			
			    			{{#each model as |evento|}}
			    			<li>
			    				<p>{{!evento.id}}
			    				{{!-- {{link-to evento.nombre 'verEvento' evento.id}} --}}
			    				
			    				{{#link-to 'verEvento' evento.id}}
			    					{{evento.nombre}}
			    				{{/link-to}}
			    				   				</p>
			    			</li>
			    			{{/each}}
			    			

			    				
			    		</form>
			    	</div>
	    		</div>
    		</div>
    	</div>
    </div>

